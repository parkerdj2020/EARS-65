// ----------- Javascript Dictionaries -----------------------
// Special type of list where each item has a name or "key"
// Use {} instead of []
// Items can include lists
var fairchildAddress = {
  streetNumber: 19,
  streetName: 'Fayerweather'
}

print('You are at ' + fairchildAddress.streetNumber + ' ' + fairchildAddress.streetName);


// Can access each item in dictionary with key using dot notation
